funcion entero mundo() hacer
    imprimir("mundo");
    booleano ver = falso;
    si(ver) entonces
        retornar 1;
    si_no
        retornar 0;
    fin_si
fin_funcion

funcion entero hola() hacer
    imprimir("hola");
    retornar mundo();
fin_funcion

funcion entero select() hacer
    entero a = 0;
    seleccionar(a) entre
        caso 5:
            retornar 10;
        caso 10:
            retornar 15;
        defecto:
            retornar 0;
    fin_seleccionar
fin_funcion

